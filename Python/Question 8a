import random

random_numbers = [random.randint(1, 9999) for _ in range(20)]

# Filter and display odd numbers with 2 or 4 digits
odd_numbers_of_length_2_and_4 = [num for num in random_numbers if num % 2 != 0 and (len(str(num)) == 2 or len(str(num)) == 4)]

print("Random Numbers:", random_numbers)
print("Odd Numbers of Length 2 and 4:", odd_numbers_of_length_2_and_4)
